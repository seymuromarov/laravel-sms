  "description": "Laravel api for sending sms for azerbaijan providers.",
  "type": "laravel",
  "keywords": [
    "azerbaijan",
    "sms",
    "send",
    "provider",
    "baku",
    "laravel",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Seymur Omarov",
      "email": "omarov.seymur@outlook.com"
    }
  ],
